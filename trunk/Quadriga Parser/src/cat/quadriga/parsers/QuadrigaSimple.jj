// Coses de quadriga a partir d'aqui:

void QuadrigaUnit():
{
  String packa;}
{
  packa = QuadrigaPackageDeclaration()

  ( QuadrigaDeclaration() [";"] )+    

  ( < "\u001a" > )?
  ( <STUFF_TO_IGNORE_BIS: ~[]> )?
  <EOF>}

BaseType QuadrigaEntity():
{
  List<ParametrizedClass.ParameterClass> components = null;
  Token t;}{
  t="Entity"
  [components = TypeArguments()]
  {
    if(components != null) {
      CodeZone cz = new CodeZoneClass(t,file);      Set<QuadrigaComponent> componentSet = new HashSet<QuadrigaComponent>();
      for(ParametrizedClass.ParameterClass param : components) {
        if(param.bound != ParametrizedClass.ParameterClass.Bound.NO) {
          errorLog.insertError("Un paràmetre d'una entitat no pot tenir \"super\" ni \"extends\"", cz);        } else if(param.base instanceof QuadrigaComponent) {
          QuadrigaComponent component = (QuadrigaComponent)param.base;
          if(componentSet.contains(component)) {
            errorLog.insertError(component.getBinaryName() + " repetit", cz);          } else {
            componentSet.add(component);          }        } else {
          errorLog.insertError(param.base.getBinaryName() + " no és un component", cz);        }      }

      return QuadrigaEntity.getEntity(componentSet);    }    

    return QuadrigaEntity.baseEntity;  }}

String QuadrigaPackageDeclaration():
{
  List<Token> name;}
{
  "@quadriga" name = Name() [";"]
  {
    String res = "";
    boolean first = true;
    for(Token t: name) {
      if(!first) {
        res += "." + t.image;      } else {
        res = t.image;
        first = false;      }    }
    currentFile = res.replace('.', '/');
    symbolTable.addPackage("javax.vecmath");
    return res;  }}

void QuadrigaDeclaration():
{}
{
  JavaImport()
| ComponentDeclaration()
| PrototypeDeclaration()
| SystemDeclaration()
| ThreadDeclaration()}

void JavaImport():
{
  List<Token> name;}
{
  "@java"  name = Name()
  {
    String res = "";
    boolean first = true;
    for(Token t: name) {
      if(!first) {
        res += "." + t.image;
      } else {
        res = t.image;
        first = false;
      }
    }
	try {
	  Class<?> clazz = Class.forName(res);
	  BaseSymbol symbol = new TypeSymbol(ClassOrInterfaceTypeRef.getFromClass(clazz));
	  symbolTable.addGlobalSymbol(symbol);
    } catch (ClassNotFoundException e) {
	  //TODO revisar això
	  BaseSymbol symbol = new TypeSymbol(UnknownType.empty);
	  symbolTable.addGlobalSymbol(symbol);
    }
  }}

void ComponentDeclaration():
{
  Token t;
  QuadrigaComponent component = null;
  List<Token> name;
  BaseType type;
  ExpressionNode initialization = null;
}
{
  "@component" 
  (
    LOOKAHEAD( 2 ) 
    t= < IDENTIFIER >
    {
      IncompleteComponent iComponent = new IncompleteComponent(currentFile.replace('/','.'), t.image);    }
    "{"
        (
          // Dependencies
          "*" name = Name() [";"]
          {
            iComponent.addDependency(name, symbolTable, errorLog,file);            }
        )*
        ( type = Type()
          t = < IDENTIFIER >
          [ "=" initialization = VariableInitializer(type) ] [";"]
          {
            iComponent.addField(new QuadrigaComponent.ComponentField(
                                           type, t.image, initialization
                                                                ),
                                errorLog,
                                new CodeZoneClass(t,file)
                                 );          }         )*
    "}"
    {
      component = new CompleteComponent(iComponent);    }
  |
    name = Name()
    {
      String res = "";
      String dependency = null;
      for(int i = 0; i < name.size(); i++) {
        if(i == name.size() - 1)
        {
          dependency = res;
        }
        if(i != 0) {
          res += "." + name.get(i).image;
        } else {
          res = name.get(i).image;
        }
      }
      if(name.size() == 1)      {
        component = new ProxyComponent(currentFile.replace('/','.') + '.' + res);      } else      {
        BaseSymbol symbol = symbolTable.findSymbol(res);
        if(symbol != null) {
          if(symbol instanceof TypeSymbol
             && ((TypeSymbol)symbol).type instanceof QuadrigaComponent)
          {
            component = null; //Ja hi és a la taula de símbols          } else          {
            component = new ProxyComponent(res);
            errorLog.insertError("El símbol " + res + " no és un component",
                                 new CodeZoneClass(name.get(0),name.get(name.size()-1),file));          }        } else {          component = new ProxyComponent(res);
          addDependency(dependency);
        }
      }
    }
  )
  {
    if(component != null) {
      BaseSymbol symbol = new TypeSymbol(component);
	  symbolTable.addSymbol(symbol);
    }
  }
}

void PrototypeDeclaration():
{
  Token t;
  QuadrigaPrototype prototype = null;
  List<Token> name;
  List<ParameterClass> parameters = new LinkedList<ParameterClass>();
  ComponentAllocation componentAllocation;
  ApplyPrototypeToEntity proto;
  ExpressionNode thisEntity;
  BlockCode blockAux;
}
{
  "@prototype" 
  (
    LOOKAHEAD( 2 ) 
    t= < IDENTIFIER >
    {
      IncompletePrototype iPrototype = new IncompletePrototype(currentFile.replace('/','.'), t.image);
      symbolTable.newContext();
      symbolTable.addSymbol(new ThisSymbol(iPrototype));

      BlockCode.TmpBlockCode prevBlock = localBlock;
      localBlock = new BlockCode.TmpBlockCode(t,file);    }
    ( parameters=FormalParameters()
      {
        for(ParameterClass param : parameters)
        {
          LocalVariableSymbol localVar =
                                new LocalVariableSymbol(param.modifiers,
                                                        param.type,
                                                        param.name);
          symbolTable.addSymbol(localVar);
          localBlock.addLocalVariable(localVar);
        }
        iPrototype.parameters = parameters;
      }
    | {
        iPrototype.parameters = new Vector<ParameterClass>();      }
    ) 
    [ t="extends"
      {        thisEntity = new LocalVarAccess(
                                   (ThisSymbol)symbolTable.findSymbol("this"),
                                   new CodeZoneClass(t,file)
                                   );
      }
      proto=EntityInitialization(thisEntity)
      {        localBlock.addStatement(proto);      }      ( "," proto=EntityInitialization(thisEntity)        {          localBlock.addStatement(proto);        }      )*
    ] 
    "{"
        (          componentAllocation = ComponentDeclarator() [";"]
          {
            thisEntity = new LocalVarAccess(
                                   (ThisSymbol)symbolTable.findSymbol("this"),
                                   componentAllocation
                                   );
            localBlock.addStatement( new AddComponentNode(componentAllocation,
                                                          thisEntity,
                                                          componentAllocation)
                                        );          }
        |
          "@child" t="{"
              (
                {
                  ExpressionNode entityCreation, expAux;                }               (
                 LOOKAHEAD(Expression() ":")                 expAux = Expression() t=":"
                 {
                   CodeZoneClass czaux = new CodeZoneClass(expAux,t);
                   thisEntity = new LocalVarAccess(
                           (ThisSymbol)symbolTable.findSymbol("this"),
                           czaux
                           );
                   entityCreation = new NewEntity(thisEntity,expAux,czaux);                 }
               |                 {
                   CodeZoneClass czaux = new CodeZoneClass(t,file);
                   thisEntity = new LocalVarAccess(
                           (ThisSymbol)symbolTable.findSymbol("this"),
                           czaux
                           );
                   entityCreation = new NewEntity(thisEntity,null,czaux);                 }
               )

               proto = EntityInitialization(entityCreation) [t=";"]
               {
                 localBlock.addStatement(proto);               }
              )*
          "}"
        | "@init" blockAux=Block()
          {
            localBlock.addStatement(blockAux);          }
        )*
    t="}"
    {
      localBlock.setEndToken(t);
      iPrototype.initializations = localBlock.transformToBlockCode();
      localBlock = prevBlock;      symbolTable.deleteContext();
      
      prototype = new CompletePrototype(iPrototype);    }
  |
    name = Name()
    {
      String res = "";
      String dependency = null;
      for(int i = 0; i < name.size(); i++) {
        if(i == name.size() - 1)
        {
          dependency = res;
        }
        if(i != 0) {
          res += "." + name.get(i).image;
        } else {
          res = name.get(i).image;
        }
      }
      if(name.size() == 1)
      {
        prototype = new ProxyPrototype(currentFile.replace('/','.') + '.' + res);
      } else
      {
        BaseSymbol symbol = symbolTable.findSymbol(res);
        if(symbol != null) {
          if(symbol instanceof TypeSymbol
             && ((TypeSymbol)symbol).type instanceof QuadrigaPrototype)
          {
            prototype = null; //Ja hi és a la taula de símbols
          } else
          {
            prototype = new ProxyPrototype(res);
            errorLog.insertError("El símbol " + res + " no és un prototip",
                                 new CodeZoneClass(name.get(0),name.get(name.size()-1),file));
          }
        } else {
          prototype = new ProxyPrototype(res);
          addDependency(dependency);
        }
      }
    }
  )
  {
    BaseSymbol symbol = new TypeSymbol(prototype);
    symbolTable.addSymbol(symbol);
  }
}

