void ClassOrInterfaceBody(boolean isInterface, List<PrintInfo> out):
{
}
{
  "{"
  {
    out.add(new PrintString("{"));
    out.add(PrintInitTab.getInstance());
    out.add(PrintIntro.getInstance());
  }
  ( ClassOrInterfaceBodyDeclaration(isInterface, out)
    {
      out.add(PrintIntro.getInstance());
    }
  )*
  "}"
  {
    out.add(PrintEndTab.getInstance());
    out.add(PrintIntro.getInstance());
    out.add(new PrintString("}"));
    out.add(PrintIntro.getInstance());
  }
}

void ClassOrInterfaceBodyDeclaration(boolean isInterface, List<PrintInfo> out):
{
  //TODO
  boolean isNestedInterface = false;
  int modifiers;
}
{
  (
    LOOKAHEAD(2)
    Initializer(out)
    {
       if (isInterface)
          throw new ParseException("An interface cannot have initializers");
    }
  |
    modifiers = Modifiers(out) // Just get all the modifiers out of the way. If you want to do
                // more checks, pass the modifiers down to the member
    (
        ClassOrInterfaceDeclaration(modifiers, out)
      |
        EnumDeclaration(modifiers, out)
      |
        LOOKAHEAD( [ TypeParameters() ] <IDENTIFIER> "(" )
        ConstructorDeclaration(out)
      |
        LOOKAHEAD( Type() <IDENTIFIER> ( "[" "]" )* ( "," | "=" | ";" ) )
        FieldDeclaration(modifiers, out)
      |
        MethodDeclaration(modifiers, out)
      |
        AnnotationTypeDeclaration(modifiers, out)
    )
  |
    ";"
  )
}

void FieldDeclaration(int modifiers, List<PrintInfo> out):
{
  VarAndInit v;
  BaseType tipus;
}
{
  // Modifiers are already matched in the caller
  tipus = Type()
  {
    out.add(PrintWhiteSpace.getInstance());
  }
  v = VariableDeclarator(modifiers, tipus)
  ( ","
    {
      //TODO
    }
    v = VariableDeclarator(modifiers, tipus)
  )* ";"
  {
    out.add(new PrintString(";"));
  }
}

VarAndInit VariableDeclarator(int modifiers, BaseType type):
{
  VarAndInit vai;
  ExpressionNode initialization;
  ExpressionNode leftHand;
}
{
  vai = VariableDeclaratorId(type)
  {
    vai.modifiers = modifiers;
    if(localBlock != null) //Local var
    {
      LocalVariableSymbol localVar = new LocalVariableSymbol(modifiers, vai.type, vai.name.image);
      symbolTable.addSymbol(localVar);
      localBlock.addLocalVariable(localVar);

      leftHand = new LocalVarAccess(localVar, new CodeZoneClass(vai.name,file));
    } else {
      //Field
      leftHand = new ProxyDataAccess("Proxy field: " + vai.name, new CodeZoneClass(vai.name,file));    }  }
  [ "="
    initialization = VariableInitializer(type)
    {
      vai.init = new AssigmentStatementNode(
                                      AssigmentStatementNode.Operator.ASSIGN,
                                      leftHand,
                                      initialization);    }
  ]
  {    return vai;
  }
}

VarAndInit VariableDeclaratorId(BaseType type):
{
  Token t;
  VarAndInit vai = new VarAndInit();
}
{
  t=<IDENTIFIER>
  {
    vai.name = t;
    vai.type = type;
  }
  ( "[" "]" 
    {
      vai.type = new ArrayType(vai.type);
    }
  )*
  {
    return vai;  }
}

ExpressionNode VariableInitializer(BaseType base):
{
  ExpressionNode result;
}
{
  (
    result = ArrayInitializer(base)
  |
    result = Expression()
  )
  {    return result;
  }
}

ExpressionNode ArrayInitializer(BaseType base):
{
  ExpressionNode aux;
  List<ExpressionNode> listOfInits = new LinkedList<ExpressionNode>();
  Token t, t2;
  if(base instanceof ArrayType) {
    base = ((ArrayType)base).base;  }
}
{
  t="{"
  [ aux = VariableInitializer(base)
    {
      listOfInits.add(aux);
    }
    ( LOOKAHEAD(2) "," aux = VariableInitializer(base)
      {
        listOfInits.add(aux);
      }
    )*
  ]
  [ "," ]
  t2="}"
  {
    ExpressionNode[] arraySize = {new LiteralData.IntegerLiteral(listOfInits.size())};

    ArrayAllocationExpressionNode allocation = new ArrayAllocationExpressionNode(
                                                   base,
                                                   arraySize,
                                                   1,
                                                   new CodeZoneClass(t, t2,file)
                                                   ); 
    return new ArrayInitializerNode(allocation,
                                    listOfInits.toArray(
                                          new ExpressionNode[listOfInits.size()])
                                    );
  }
}

void MethodDeclaration(int modifiers, List<PrintInfo> out):
{
  BlockCode bc;
  List<PrintInfo> other = new LinkedList<PrintInfo>();

  symbolTable.newContext();
  symbolTable.addSymbol(new ThisSymbol());
}
{
  // Modifiers already matched in the caller!
  [ TypeParameters(out) ]
  {
    out.add(PrintWhiteSpace.getInstance());
  }
  ResultType()
  {
    out.add(PrintWhiteSpace.getInstance());
  }
  MethodDeclarator(out)
  [ "throws"
    {
      out.add(PrintWhiteSpace.getInstance());
      out.add(new PrintString("throws"));
      out.add(PrintWhiteSpace.getInstance());
    }
    NameList()
  ]
  ( bc = Block()
    {
      String treeString = bc.treeStringRepresentation();
      out.addAll( MegaPrinter.getInstance().format(treeString) );
    }
  | ";"
    {
      out.add(new PrintString(";"));
    }
  )
  {
    symbolTable.deleteContext();  }
}

void MethodDeclarator(List<PrintInfo> out):
{
  Token t;
  List<ParameterClass> params;
}
{
  t=<IDENTIFIER>
  {
    out.add(new PrintString(t.image));
  }
  /* Això servia en el Java vell
  ( "[" "]"
    {
      out.add(new PrintString("["));
      out.add(new PrintString("]"));
    }
  )*
  */
  params = FormalParameters()
  {
    for(ParameterClass param : params)
    {
      symbolTable.addSymbol(new LocalVariableSymbol(param.modifiers,
                                                    param.type,
                                                    param.name));
    }
  }
  {
    out.add(PrintIntro.getInstance());
    out.add(new PrintString("Params"));
    for(ParameterClass param : params)
    {
      out.add(PrintIntro.getInstance());
      out.add(new PrintString(param.name + ": " + param.type));
    }
    out.add(PrintIntro.getInstance());
  }
}

List<ParameterClass> FormalParameters():
{
  List<ParameterClass> params = new LinkedList<ParameterClass>();
  ParameterClass param;
}
{
  "("
  [ param = FormalParameter()
    {
      params.add(param);    }
    ( ","
    param = FormalParameter()
    {
      params.add(param);
    }
    )*
  ]
  ")"
  {
    return params;
  }
}

ParameterClass FormalParameter():
{
  int modifiers;
  BaseType type;
  List<PrintInfo> out = new LinkedList<PrintInfo>();
  VarAndInit vai;
  ExpressionNode init = null;
  boolean varargs = false;
  Token t;
  String semantic = null;
}
{
  // danson, added
  //          [ "final" | Annotation() ]
  // See Java Language Specification, 3rd Edition, section 8.4.1
  modifiers = Modifiers(out)
  [ "final"
    {
      modifiers &= FINAL;
    }
  | Annotation(out)
    {      //caca
    }
    
  ]
  type = Type()
  [ "..."
  {
    type = new ArrayType(type);
    varargs = true;
  }
  ]
  vai = VariableDeclaratorId(type)

  //La semàntica fa que quadriga hi posi el paràmetre. Per tant no es pot posar
  //per defecte
  [ "=" init = Expression() 
  | ":" t = < IDENTIFIER > { semantic = t.image; } ]
  {
    return new ParameterClass(
      new CodeZoneClass(vai.name,file),
      vai.type,
      vai.name.image,
      varargs,
      modifiers,
      init,
      semantic);  }
}

void ConstructorDeclaration(List<PrintInfo> out):
{
  Token t;
  out.add(PrintWhiteSpace.getInstance());
  StatementNode bsn;

  List<ParameterClass > params;

  symbolTable.newContext();
  symbolTable.addSymbol(new ThisSymbol());
}
{
  [ TypeParameters(out) ]
  // Modifiers matched in the caller
  t=<IDENTIFIER>
  {
    out.add(new PrintString(t.image));
  }
  params = FormalParameters()
  {
    for(ParameterClass param : params)
    {
      symbolTable.addSymbol(new LocalVariableSymbol(param.modifiers,
                                                    param.type,
                                                    param.name));
    }
  }
  {
    out.add(PrintIntro.getInstance());
    out.add(new PrintString("Params"));
    for(ParameterClass param : params)
    {
      out.add(PrintIntro.getInstance());
      out.add(new PrintString(param.name + ": " + param.type));    }
    out.add(PrintIntro.getInstance());  }
  [ "throws"
    {
      out.add(PrintWhiteSpace.getInstance());
      out.add(new PrintString("throws"));
      out.add(PrintWhiteSpace.getInstance());
    }
  NameList()
  ]
  "{"
  {
    out.add(PrintWhiteSpace.getInstance());
    out.add(new PrintString("{"));
    out.add(PrintInitTab.getInstance());
    out.add(PrintIntro.getInstance());
  } 
  [ LOOKAHEAD(ExplicitConstructorInvocation())
       ExplicitConstructorInvocation(out)
  ]
  ( bsn = BlockStatement()
    {
      String treeString = bsn.treeStringRepresentation();
      out.addAll( MegaPrinter.getInstance().format(treeString) );
    }
  )*
  "}"
  {
    while(out.get(out.size()-1) == PrintIntro.getInstance())
      out.remove(out.size()-1);
    if(out.get(out.size()-1) != PrintInitTab.getInstance()) {
      out.add(PrintEndTab.getInstance());
      out.add(PrintIntro.getInstance());
    } else {
      out.add(PrintEndTab.getInstance());
    }
    out.add(new PrintString("}"));
  }

  {
    symbolTable.deleteContext();  }
}

void ExplicitConstructorInvocation(List<PrintInfo> out):
{
  Token t;
}
{
  ( t=<IDENTIFIER> "."
    {
      out.add(new PrintString(t.image));
      out.add(new PrintString("."));
    }
  )*
  [ LOOKAHEAD(2) "this" "."
    {
      out.add(new PrintString("this"));
      out.add(new PrintString("."));
    }
  ]
  [ TypeArguments() ]
  ( "this"
    {
      out.add(new PrintString("this"));
    }
  | "super"
    {
      out.add(new PrintString("super"));
    }
  ) Arguments() ";"
    {
      out.add(new PrintString(";"));
      out.add(PrintIntro.getInstance());
    }
}

void Initializer(List<PrintInfo> out):
{}
{
  [ "static"
  {
    {
      out.add(new PrintString("static"));
      out.add(PrintWhiteSpace.getInstance());
    }
  }
  ] Block() //TODO
}


/*
 * Type, name and expression syntax follows.
 */

BaseType Type():
{
  BaseType t;
}
{
  (
    t=QuadrigaEntity()
  | 
    LOOKAHEAD(2) t = ReferenceType()
  |
    t = PrimitiveType()
  )
  {
    return t;
  }
}

BaseType ReferenceType():
{
  BaseType type;
}
{
  (
    
    type = PrimitiveType()
    ( LOOKAHEAD(2) "[" "]"
      {
        type = new ArrayType(type);
      }
    )+
  |
    type = ClassOrInterfaceType() 
    ( LOOKAHEAD(2) "[" "]"
      {
        type = new ArrayType(type);
      }
    )*
  )
  {    return type;
  }
}

BaseType ClassOrInterfaceType():
{
  Token t, first, last;
  List<String> aux = new LinkedList<String>();
  List<ParametrizedClass.ParameterClass> arguments = null;
  List<PrintInfo> out = new LinkedList<PrintInfo>();
  BaseType type;
}
{
  t=<IDENTIFIER>
  {
    aux.add(t.image);
    first = last = t;
  }
  (     LOOKAHEAD(2)arguments = TypeArguments()
  |
   {
     arguments = null;   }
  )
  ( LOOKAHEAD(2) "." t=<IDENTIFIER>
    {
      aux.add(t.image);
      last = t;
    }
    [ LOOKAHEAD(2) arguments = TypeArguments()
      //TODO reformular tot això
    ]
  )*
  {
    CodeZone cz = new CodeZoneClass(first, last, file);
    String name = aux.get(0);
    for(int i = 1; i < aux.size(); i++) {
	  name += "." + aux.get(i);    }
    BaseSymbol symbol = symbolTable.findSymbol(name);
    BaseType result = null;
    if(symbol != null) {
      if(symbol instanceof TypeSymbol) {
        result = ((TypeSymbol)symbol).type;      } else {
        errorLog.insertError("El simbol \"" + name + "\" no és un tipus.", cz);
        return UnknownType.empty;       }    } else {
      errorLog.insertWarning("No s'ha trobat la classe \"" + name + "\".", cz);
      return new UnknownType(name);    }

    //Type Arguments
    if(arguments != null) {
      if(result instanceof ClassOrInterfaceTypeRef) {
        result = new ParametrizedClass((ClassOrInterfaceTypeRef)result, arguments);
      } else {
        result = new ParametrizedClass(result, arguments);      }    }
    return result;  }
}

List<ParametrizedClass.ParameterClass> TypeArguments():
{
  List<ParametrizedClass.ParameterClass> arguments = new LinkedList<ParametrizedClass.ParameterClass>();
  ParametrizedClass.ParameterClass argument;
}
{
   "<"
   argument = TypeArgument()
   {
     arguments.add(argument);   }
   ( ","
     argument = TypeArgument()
     {
       arguments.add(argument);
     }
   )* ">"
   {
     return arguments;
   }
}

ParametrizedClass.ParameterClass TypeArgument():
{
  ParametrizedClass.ParameterClass typeArgument;
  BaseType base;
}
{
 ( 
   base = ReferenceType()
   {
     typeArgument = new ParametrizedClass.ParameterClass(base);   }
 |
   "?"
   (
     typeArgument = WildcardBounds()
   |
     {
       typeArgument = new ParametrizedClass.ParameterClass();     }
   )
 )
 {   return typeArgument;
 }
}

ParametrizedClass.ParameterClass WildcardBounds():
{
  List<PrintInfo> out = new LinkedList<PrintInfo>();
  ParametrizedClass.ParameterClass typeArgument;
  BaseType base;
}
{
 (
   "extends" base=ReferenceType()
   {
     typeArgument = new ParametrizedClass.ParameterClass(base,ParametrizedClass.ParameterClass.Bound.EXTENDS);
   }
 |
   "super" base=ReferenceType()
   {
     typeArgument = new ParametrizedClass.ParameterClass(base,ParametrizedClass.ParameterClass.Bound.SUPER);
   }
 )
 {
   return typeArgument;
 }
}


PrimitiveTypeRef PrimitiveType():
{
  Token t;
}
{
  (
      t="boolean"
    |
      t="char"
    |
      t="byte"
    |
      t="short"
    |
      t="int"
    |
      t="long"
    |
      t="float"
    |
      t="double"
  )
  {
    return PrimitiveTypeRef.getFromName(t.image);
  }
}

BaseType ResultType():
{
  BaseType type;
  Token t;
}
{
  (   
      t="void"
      {
        type = PrimitiveTypeRef.getFromName(t.image);
      }
    |
      type = Type()
    )
    {      return type;
    }
}

List<Token> Name():
/*
 * A lookahead of 2 is required below since "Name" can be followed
 * by a ".*" when used in the context of an "ImportDeclaration".
 */
{
  List<Token> tokens = new LinkedList<Token>();
  Token t;
}
{
  t=<IDENTIFIER>
  {
    tokens.add(t);
  }
  ( LOOKAHEAD(2) "." t=<IDENTIFIER>
    {
      tokens.add(t);
    }
  )*
  {    return tokens;
  }
}

List<List<Token>> NameList():
{
  List<Token> aux;
  List<List<Token>> names = new LinkedList<List<Token>>();
}
{
  aux = Name()
  {
    names.add(aux);  }
  ( ","
    Name()
    {      names.add(aux);
    }
  )*
  {    return names;
  }
}


/*
 * Expression syntax follows.
 */

ExpressionNode Expression():
/*
 * This expansion has been written this way instead of:
 *   Assignment() | ConditionalExpression()
 * for performance reasons.
 * However, it is a weakening of the grammar for it allows the LHS of
 * assignments to be any conditional expression whereas it can only be
 * a primary expression.  Consider adding a semantic predicate to work
 * around this.
 */
{
  ExpressionNode finalNode, auxNode;
  AssigmentStatementNode.Operator operator; 
}
{
  finalNode = ConditionalExpression()
  [
    LOOKAHEAD(2)
    operator = AssignmentOperator()
    auxNode = Expression()
    {
      finalNode = new AssigmentExpressionNode(operator, finalNode, auxNode);    }
  ]

  {    return finalNode;
  }
}

AssigmentStatementNode.Operator AssignmentOperator():
{
  AssigmentStatementNode.Operator op;
}
{
  ( "="    { op = AssigmentStatementNode.Operator.ASSIGN; }
  | "*="   { op = AssigmentStatementNode.Operator.STARASSIGN; }
  | "/="   { op = AssigmentStatementNode.Operator.SLASHASSIGN; }
  | "%="   { op = AssigmentStatementNode.Operator.REMASSIGN; }
  | "+="   { op = AssigmentStatementNode.Operator.PLUSASSIGN; }
  | "-="   { op = AssigmentStatementNode.Operator.MINUSASSIGN; }
  | "<<="  { op = AssigmentStatementNode.Operator.LSHIFTASSIGN; }
  | ">>="  { op = AssigmentStatementNode.Operator.RSIGNEDSHIFTASSIGN; }
  | ">>>=" { op = AssigmentStatementNode.Operator.RUNSIGNEDSHIFTASSIGN; }
  | "&="   { op = AssigmentStatementNode.Operator.ANDASSIGN; }
  | "^="   { op = AssigmentStatementNode.Operator.XORASSIGN; }
  | "|="   { op = AssigmentStatementNode.Operator.ORASSIGN; }
  )
  {    return op;
  }
}

ExpressionNode ConditionalExpression():
{
  ExpressionNode finalNode, auxNode1, auxNode2;
}
{
  finalNode = ConditionalOrExpression()
  
  [ "?" auxNode1=Expression() ":" auxNode2=Expression()
    {
      finalNode = new TernaryExpressionNode(finalNode, auxNode1, auxNode2);
    }
  ]
  {
    return finalNode;  }
}

ExpressionNode ConditionalOrExpression():
{
  ExpressionNode finalNode, auxiliarNode;
  LogicalOperation.Operator operator;
}
{
  finalNode = ConditionalAndExpression()
  ( "||" { operator = LogicalOperation.Operator.OR; }
    auxiliarNode = ConditionalAndExpression()
    {
      finalNode = new LogicalOperation(operator,finalNode,auxiliarNode);
    }
  )*
  {
    return finalNode;
  }
}

ExpressionNode ConditionalAndExpression():
{
  ExpressionNode finalNode, auxiliarNode;
  LogicalOperation.Operator operator;
}
{
  finalNode = InclusiveOrExpression()
  ( "&&" { operator = LogicalOperation.Operator.AND; }
    auxiliarNode = InclusiveOrExpression()
    {
      finalNode = new LogicalOperation(operator,finalNode,auxiliarNode);
    }
  )*
  {
    return finalNode;
  }
}

ExpressionNode InclusiveOrExpression():
{
  ExpressionNode finalNode, auxiliarNode;
  MathematicOperation.Operator operator;
}
{
  finalNode = ExclusiveOrExpression()
  ( "|" { operator = MathematicOperation.Operator.BIT_OR; }
    auxiliarNode = ExclusiveOrExpression()
    {
      finalNode = new MathematicOperation(operator,finalNode,auxiliarNode);
    }
  )*
  {
    return finalNode;
  }
}

ExpressionNode ExclusiveOrExpression():
{
  ExpressionNode finalNode, auxiliarNode;
  MathematicOperation.Operator operator;
}
{
  finalNode = AndExpression()
  ( "^" { operator = MathematicOperation.Operator.BIT_XOR; }
    auxiliarNode = AndExpression()
    {
      finalNode = new MathematicOperation(operator,finalNode,auxiliarNode);
    }
  )*
  {
    return finalNode;
  }
}

ExpressionNode AndExpression():
{
  ExpressionNode finalNode, auxiliarNode;
  MathematicOperation.Operator operator;
}
{
  finalNode = EqualityExpression()
  ( "&" { operator = MathematicOperation.Operator.BIT_AND; }
    auxiliarNode = EqualityExpression()
    {
      finalNode = new MathematicOperation(operator,finalNode,auxiliarNode);
    }
  )*
  {
    return finalNode;
  }
}

ExpressionNode EqualityExpression():
{
  ExpressionNode finalNode, auxiliarNode;
  BooleanOperation.Operator operator;
}
{
  finalNode = InstanceOfExpression()
  (
    ( "==" { operator = BooleanOperation.Operator.EQ; }
    | "!=" { operator = BooleanOperation.Operator.NEQ; }
    )
    auxiliarNode = InstanceOfExpression()
    {
      finalNode = new BooleanOperation(operator,finalNode,auxiliarNode);
    }
  )*
  {    return finalNode;
  }
}

ExpressionNode InstanceOfExpression():
{
  ExpressionNode finalNode, auxiliarNode;
  BooleanOperation.Operator operator;
  Token t;
  BaseType type;
}
{
  finalNode = RelationalExpression()
  [ t="instanceof" { operator = BooleanOperation.Operator.INSTANCEOF; }
    type = Type()
    {
      auxiliarNode = new TypeDataAccess(type,new CodeZoneClass(t,file));
      finalNode = new BooleanOperation(operator,finalNode,auxiliarNode);    }
  ]
  {
    return finalNode;
  }
}

ExpressionNode RelationalExpression():
{
  ExpressionNode finalNode, auxiliarNode;
  BooleanOperation.Operator operator;
}
{
  finalNode = ShiftExpression()
  (
    ( "<" { operator = BooleanOperation.Operator.LT; }
    | ">" { operator = BooleanOperation.Operator.GT; }
    | "<=" { operator = BooleanOperation.Operator.LE; }
    | ">=" { operator = BooleanOperation.Operator.GE; }
    )
    auxiliarNode = ShiftExpression()
    {
      finalNode = new BooleanOperation(operator,finalNode,auxiliarNode);
    }
  )*
  {
    return finalNode;
  }
}

ExpressionNode ShiftExpression():
{
    ExpressionNode finalNode, auxiliarNode;
    MathematicOperation.Operator operator;
}
{
  finalNode = AdditiveExpression()
  (
    ( "<<" { operator = MathematicOperation.Operator.LEFT_SHIFT; }
    | RSIGNEDSHIFT()
           { operator = MathematicOperation.Operator.RIGHT_SHIFT; }
    | RUNSIGNEDSHIFT()
           { operator = MathematicOperation.Operator.RIGHT_UNSIGNED_SHIFT; }
    )
    auxiliarNode = AdditiveExpression()
    {
      finalNode = new MathematicOperation(operator,finalNode,auxiliarNode);
    }
  )*
  {
    return finalNode;
  }
}

ExpressionNode AdditiveExpression():
{
    ExpressionNode finalNode, auxiliarNode;
    MathematicOperation.Operator operator;
}
{
  finalNode = MultiplicativeExpression()
  (
    (   "+" { operator = MathematicOperation.Operator.ADD; }
      | "-" { operator = MathematicOperation.Operator.SUB;  }
    )
    auxiliarNode = MultiplicativeExpression()
    {
      finalNode = new MathematicOperation(operator,finalNode,auxiliarNode);
    }
  )*
  {
    return finalNode;
  }
}

ExpressionNode MultiplicativeExpression():
{
    List<PrintInfo> out = new LinkedList<PrintInfo>();
    ExpressionNode finalNode, auxiliarNode;
    MathematicOperation.Operator operator;
}
{
  finalNode = UnaryExpression()
  (
    (   "*" { operator = MathematicOperation.Operator.MUL; }
	  | "/" { operator = MathematicOperation.Operator.DIV; }
	  | "%" { operator = MathematicOperation.Operator.MOD; }
	)
    auxiliarNode = UnaryExpression()
    {
      finalNode = new MathematicOperation(operator,finalNode,auxiliarNode);    }
  )*
  {    return finalNode;
  }
}

ExpressionNode UnaryExpression():
{
  ExpressionNode finalNode;
  UnaryOperation.Operator operator;
  Token t;
}
{
  (
    ( t="+" { operator = null; }
    | t="-" { operator = UnaryOperation.Operator.NEGATE; } )
    finalNode = UnaryExpression()
    {
      if(operator != null)
        finalNode = new UnaryOperation( operator, finalNode, t );    }
  | finalNode = PreIncrementExpression()
  | finalNode = PreDecrementExpression()
  | finalNode = UnaryExpressionNotPlusMinus()
  )
  
  {
    return finalNode;
  }
}

ExpressionNode PreIncrementExpression():
{
  ExpressionNode node;
  Token t;
}
{
  t="++"
  node = PrimaryExpression()
  
  {
    return new UnaryOperation( UnaryOperation.Operator.PRE_INC, node, t );
  }
}

ExpressionNode PreDecrementExpression():
{
  ExpressionNode node;
  Token t;
}
{
  t = "--"
  node = PrimaryExpression()
  
  {
    return new UnaryOperation( UnaryOperation.Operator.PRE_DEC, node, t );
  }
}

ExpressionNode UnaryExpressionNotPlusMinus():
{
  List<PrintInfo > out = new LinkedList<PrintInfo >();
  ExpressionNode finalNode;
  UnaryOperation.Operator operator;
  Token t;
}
{
  (
    ( t = "~" { operator = UnaryOperation.Operator.BIT_COMP; }
    | t = "!" { operator = UnaryOperation.Operator.NOT; }
    )
     finalNode = UnaryExpression()
     {
       finalNode = new UnaryOperation( operator, finalNode, t );     }
  |
    LOOKAHEAD( CastLookahead() )
    finalNode = CastExpression()
  |
    finalNode = PostfixExpression()
  )
  
  {
    return finalNode;
  }
}

// This production is to determine lookahead only.  The LOOKAHEAD specifications
// below are not used, but they are there just to indicate that we know about
// this.
void CastLookahead():
{
}
{
  LOOKAHEAD(2)
  "(" PrimitiveType()
|
  LOOKAHEAD("(" Type() "[")
  "(" Type() "[" "]"
|
  "(" Type() ")" ( "~" | "!" | "(" | <IDENTIFIER> | "this" | "super" | "new" | Literal() )
}

ExpressionNode PostfixExpression():
{
  ExpressionNode finalNode;
  Token t;
  UnaryOperation.Operator operator;
}
{
  finalNode = PrimaryExpression()
  [ ( t="++" {operator = UnaryOperation.Operator.POST_INC;} | t="--" {operator = UnaryOperation.Operator.POST_DEC;})
    {
      finalNode = new UnaryOperation(operator, finalNode, t);    }
  ]

  {    return finalNode;
  }
}

ExpressionNode CastExpression():
{
  ExpressionNode finalNode;
  BaseType type;
  Token t;

  List<PrintInfo> out = new LinkedList<PrintInfo>();
}
{
  (
    LOOKAHEAD("(" PrimitiveType())
    t="(" type=Type() ")" finalNode = UnaryExpression()
  |
    t="(" type=Type() ")" finalNode = UnaryExpressionNotPlusMinus()
  )
  {
    return new CastExpressionNode(t, type, finalNode);
  }
}

ExpressionNode PrimaryExpression():
{
  ExpressionNode result;
}
{
  result = PrimaryPrefix()
  ( LOOKAHEAD(2)
    result = PrimarySuffix(result)
  )*

  {    return result;
  }
}

ExpressionNode MemberSelector(ExpressionNode in):
{
  Token t, t2;
}
{
  t="." TypeArguments() t2=<IDENTIFIER>
  {
    //TODO arguments
    return new ProxyDataAccess( "parametrized " + t.image, in, new CodeZoneClass(t, t2, file));
  }
}

ExpressionNode PrimaryPrefix():
{
  List<PrintInfo> out = new LinkedList<PrintInfo>();
  Token t, t2;
  ExpressionNode result;
  List<Token> tokenList = new LinkedList<Token>();
  BaseType type;
}
{
  (
    result = Literal()
  |
    LOOKAHEAD( ( <IDENTIFIER> "." )* "this" )
    ( t=<IDENTIFIER> "."
      {
        tokenList.add(t);
      }
    )*
    t="this"
    {
      //Code zone things
      if(tokenList.size() > 0) {
        //TODO algo
        result = new ProxyExpression(tokenList.get(0).image + " ... this",
                                     new CodeZoneClass(tokenList.get(0),t,file));      } else      {
        BaseSymbol symbol = symbolTable.findSymbol("this");
        if(symbol != null) {
          result = new LocalVarAccess((LocalVariableSymbol)symbol, new CodeZoneClass(t,file));        } else {          result = new ProxyDataAccess("this", new CodeZoneClass(t,file));
        }      }
    }
  |
    t="super" "." t2=<IDENTIFIER>
    {
      //TODO
      result = new ProxyExpression("super." + t2.image, new CodeZoneClass(t, t2,file));
    }
  |
     // danson, added this part to support a construct like:
     //   Buffer.super.setDirty(true);
     // See Java Language Specification, 3rd edition, section 15.11.2.
     LOOKAHEAD( ClassOrInterfaceType() "." "super" "." <IDENTIFIER> )
     type = ClassOrInterfaceType() t="." "super" "." t2=<IDENTIFIER>
     {
       //TODO
      result = new ProxyExpression(type + ".super." + t.image, new CodeZoneClass(t, t2,file));
     }
  |
    "(" result = Expression() ")"
  |
    result = AllocationExpression(null)
  |
    LOOKAHEAD( ResultType() "." "class" )
    type = ResultType() t="." t2="class"
    {
      result = new LiteralData.ClassLiteral(type, new CodeZoneClass(t, t2,file));
    }
  |
    {
      List<Token> aux;    }
    aux = Name()
    {
      result = Utils.resolveName(symbolTable,aux,file);    }
  )
  {
    return result;  }
}

ExpressionNode PrimarySuffix(ExpressionNode in):
{
  Token t, t2;
  ExpressionNode out;
}
{
  ( 
      LOOKAHEAD("." "super" ".")
      t="." t2="super"
      {
        out = new ProxyExpression(".super",new CodeZoneClass(t,t2,file));
      }
    |
      LOOKAHEAD("." "this")
      t="." t2="this"
      {
        out = new ProxyExpression(".this",new CodeZoneClass(t,t2,file));
      }
    |
      LOOKAHEAD(2)
      "."
      out = AllocationExpression(in)
    |
      LOOKAHEAD(3)
      out = MemberSelector(in)
    |
      "["
      {
        ExpressionNode access;
      }
      access = Expression() t="]"
      {
        out = new ArrayOrComponentAccess(in, access, t);
      }
    |
      "." t = <IDENTIFIER>
      {
        out = Utils.accessToMember( in, t.image, t);
      }
    |
      {
        CallToArguments args;      }
      args = Arguments()
      {
        DataAccess method;
        if(in instanceof DataAccess)        {
          method = (DataAccess) in;        } else {          method = new ProxyDataAccess("Method Call",in);
        }
        out = new CallToMethod(method,args);
      }
  )
  {    return out;
  }
}

LiteralData Literal():
{
  Token t;
  LiteralData data;
}
{
  (    
    t=<INTEGER_LITERAL>
    {
      data = new LiteralData.IntegerLiteral(t,file);
    }
  |
    t=<FLOATING_POINT_LITERAL>
    {
      data = new LiteralData.FloatLiteral(t,file);
    }
  |
    t=<CHARACTER_LITERAL>
    {
      data = new LiteralData.CharacterLiteral(t,file);
    }
  |
    t=<STRING_LITERAL>
    {
      data = new LiteralData.StringLiteral(t,file);
    }
  |
    data = BooleanLiteral()
  |
    data = NullLiteral()
  )
  {    return data;
  }
}

LiteralData BooleanLiteral():
{
  Token t;
  LiteralData lit;
}
{
  (    
  t="true"
  {
    lit = new LiteralData.TrueLiteral(t,file);
  }
|
  t="false"
  {
    lit = new LiteralData.FalseLiteral(t,file);
  }
)
  {    return lit;
  }
}

LiteralData NullLiteral():
{
  Token t;
}
{
  t = "null"
  {
    return new LiteralData.NullLiteral(t,file);
  }
}

CallToArguments Arguments():
{
  //List<PrintInfo> other = new LinkedList<PrintInfo>();
  List<ExpressionNode> arguments = new LinkedList<ExpressionNode>();
  Map<String,ExpressionNode> argNames = new HashMap<String,ExpressionNode>();
  ExpressionNode node;
  Token t, t2, argumentName;
  CallToArguments result;
}
{
  t="("
      (
        LOOKAHEAD( < IDENTIFIER > ":" )
        argumentName = < IDENTIFIER >
        ":"
        node = VariableInitializer(UnknownType.empty)//TODO
        {
          argNames.put(argumentName.image, node);
        }
        (
          ","
          argumentName = < IDENTIFIER >
          ":"
          node = VariableInitializer(UnknownType.empty)//TODO
          {
            argNames.put(argumentName.image, node);
          }        )*
        t2=")"
        {
          result = new CallToNamedArguments(argNames, new CodeZoneClass(t,t2,file));        }

      | //non-named arguments
        node = Expression()
        {
          arguments.add(node);        }
        ( ","
          node = Expression()
          {
            arguments.add(node);
          }
        )*
        t2=")"
        {
          result = new CallToListedArguments(arguments, new CodeZoneClass(t,t2,file));
        }
      | 
        t2=")"
        {
          result = new CallToListedArguments(arguments, new CodeZoneClass(t,t2,file));
        }
      )
        
  {
    return result;
  }
}

ExpressionNode AllocationExpression(ExpressionNode in):
{
  List<PrintInfo> other  = new LinkedList<PrintInfo>()
                , other2 = new LinkedList<PrintInfo>()
                , out    = new LinkedList<PrintInfo>();

  ExpressionNode exp;
  BaseType type;
  CallToArguments args;
  Token t;
}
{
  (
    
    LOOKAHEAD(2)
    t="new" type = PrimitiveType() exp = ArrayDimsAndInits(type, t)
  |
    t="new"
    type = ClassOrInterfaceType()
    [ TypeArguments()
      //TODO
    ]
      (
        exp = ArrayDimsAndInits(type, t)
      |
        args = Arguments()

        
        (           ClassOrInterfaceBody(false, out)
           //TODO Fer inner classes?
           {
             //TODO
             exp = new ProxyExpression(out);
             exp = new UnaryProxy("new " + type.toString(), exp);
           }
        |
        {
          if(in == null) {
            exp = new AllocationExpressionNode(type, args,t);
          } else {
            exp = new AllocationExpressionNode(in,type, args,t);          }        }
        ) 
      )
  )
  {    return exp;
  }
}

/*
 * The third LOOKAHEAD specification below is to parse to PrimarySuffix
 * if there is an expression between the "[...]".
 */
ExpressionNode ArrayDimsAndInits(BaseType in, Token n):
{
  ExpressionNode result, aux;
  List<ExpressionNode> listLengths = new LinkedList<ExpressionNode>();
  int dims = 0;

  Token t;
}
{
  (
    LOOKAHEAD(2)
    ( LOOKAHEAD(2) "[" 
      aux = Expression() t="]"
      {
        listLengths.add(aux);
        dims++;
      }
    )+
    ( LOOKAHEAD(2) "[" t="]"
      {
        dims++;
      }
    )*
    {
      result = new ArrayAllocationExpressionNode(
                    in, 
                    listLengths.toArray(new ExpressionNode[listLengths.size()]),
                    dims,
                    new CodeZoneClass(n, t,file));    }
  |
    ( "[" "]"
      {
        //TODO algo aqui i no se el que...
      }
    )+
    result = ArrayInitializer(in)
  )
  {    return result;
  }
}


/*
 * Statement syntax follows.
 */

StatementNode Statement():
{
  StatementNode statement;
}
{
  (
    LOOKAHEAD(2)
    statement = LabeledStatement()
  |
    statement = AssertStatement()
  |
    statement = Block()
  |
    statement = EmptyStatement()
  |
    statement = StatementExpression() ";"
  |
    statement = SwitchStatement()
  |
    statement = IfStatement()
  |
    statement = WhileStatement()
  |
    statement = DoStatement()
  |
    statement = ForStatement()
  |
    statement = BreakStatement()
  |
    statement = ContinueStatement()
  |
    statement = ReturnStatement()
  |
    statement = ThrowStatement()
  |
    statement = SynchronizedStatement()
  |
    statement = TryStatement()
  )
  {
    return statement;  }
}

AssertStatementNode AssertStatement():
{
  ExpressionNode assertion, message=null;
  Token t, t2;
}
{
  t = "assert"
  assertion = Expression()
  [ ":"
    message = Expression()
  ]
  t2 = ";"
  {
    return new AssertStatementNode(assertion, message, new CodeZoneClass(t,t2,file));
  }
}

LabeledStatementNode LabeledStatement():
{
  Token t;
  StatementNode node;
}
{
  t=<IDENTIFIER> ":"
  node = Statement()
  {
    return new LabeledStatementNode(t, node, symbolTable, errorLog);  }
}

BlockCode Block():
{
  StatementNode blockStatementNode;
  Token t;

  BlockCode.TmpBlockCode prevBlock = localBlock;
  symbolTable.newContext();
}
{
  t = "{"
  {
    localBlock = new BlockCode.TmpBlockCode(t,file);
  }
  ( 
    blockStatementNode = BlockStatement()
    {
      localBlock.addStatement(blockStatementNode);
    }
  )*
  t="}"
  {

    localBlock.setEndToken(t);
    BlockCode block = localBlock.transformToBlockCode();


    localBlock = prevBlock;

    symbolTable.deleteContext();
    return block;
  }
}

StatementNode BlockStatement():
{
  List<PrintInfo> out = new LinkedList<PrintInfo>();
  Token t;
  StatementNode result;
}
{
  (    LOOKAHEAD( Modifiers() Type() <IDENTIFIER> )
    result = LocalVariableDeclaration() t=";"
  |
    result = Statement()
  |
    ClassOrInterfaceDeclaration(0, out)
    {
      //TODO
      result = new NoOpStatement();
    }
  )
  {    return result;
  }
}

StatementList LocalVariableDeclaration():
{
  int modifiers;
  BaseType type;
  VarAndInit var;
  List<PrintInfo> out = new LinkedList<PrintInfo>();

  List<StatementNode > initializers = new LinkedList<StatementNode >();
}
{
  modifiers=Modifiers(out) type = Type() var=VariableDeclarator(modifiers,type)
  {
    if(var.init != null) {
      initializers.add(var.init);    }  }
  
  ( ","
    var = VariableDeclarator(modifiers,type)
    {
      if(var.init != null) {
        initializers.add(var.init);
      }
    }
  )*
  {
    return new StatementList(initializers.toArray(new StatementNode[initializers.size()]));  }
}

NoOpStatement EmptyStatement():
{
  Token t;
}
{
  t = ";"
  {
    return new NoOpStatement(new CodeZoneClass(t,file));  }
}

StatementNode StatementExpression():
/*
 * The last expansion of this production accepts more than the legal
 * Java expansions for StatementExpression.  This expansion does not
 * use PostfixExpression for performance reasons.
 */
{
  ExpressionNode expression, expression2;
  Token t;
  AssigmentStatementNode.Operator assignOperator;
  StatementNode statementNode;
}
{
  (
    ( expression = PreIncrementExpression()
    | expression = PreDecrementExpression()
    )
    {
      statementNode = new StatementFromExpresion(expression);    }
  |
    expression = PrimaryExpression()
    (
      ( t = "++"
      | t = "--"
      )
      {
        expression = new UnaryOperation(UnaryOperation.Operator.PRE_INC,
                                       expression, t);
        statementNode = new StatementFromExpresion(expression);
      }
    |
      assignOperator = AssignmentOperator()
      expression2 = Expression()
      {
        statementNode = new AssigmentStatementNode(assignOperator,
                                                   expression,
                                                   expression2);      }
    |
    {
      statementNode = new StatementFromExpresion(expression);    }    ) 
  )
  {    return statementNode;
  }
}

SwitchStatementNode SwitchStatement():
{
  Token t, t2;

  ExpressionNode in, label;
  List<StatementNode> statements;
  List<SwitchStatementNode.CaseNode> cases = new LinkedList<SwitchStatementNode.CaseNode>();
  StatementNode bsn;
}
{
  t="switch" "("
  in=Expression() ")" "{"
  {
    BlockCode.TmpBlockCode prevBlock = localBlock;
    symbolTable.newContext();
    localBlock = new BlockCode.TmpBlockCode(t,file);  }
  (
    label = SwitchLabel()
    {      statements = new LinkedList<StatementNode>();
    }
    (
      bsn = BlockStatement()
      {
        statements.add(bsn);
      }
    )*
    {
      cases.add(new SwitchStatementNode.CaseNode(label,statements));
    }
  )*
  t2="}"
  {
    SwitchStatementNode node =  new SwitchStatementNode(in, cases,
                                                        localBlock.localVariables,
                                                        new CodeZoneClass(t,t2,file));
    
    localBlock = prevBlock;
    symbolTable.deleteContext();
    return node;
  }
}

ExpressionNode SwitchLabel():
{
  ExpressionNode cas;
}
{
  (    "case" cas = Expression() ":"
  |
    "default" ":"
    {
      cas = null;
    }
  )
  {
    return cas;  }
}

IfStatementNode IfStatement():
/*
 * The disambiguating algorithm of JavaCC automatically binds dangling
 * else's to the innermost if statement.  The LOOKAHEAD specification
 * is to tell JavaCC that we know what we are doing.
 */
{
  StatementNode node1, node2 = null;
  ExpressionNode expression;
  Token t;
}
{
  t="if" "("
  expression = Expression()
  ")"
  {
    symbolTable.newContext();
  }
  node1 = Statement()
  {
    symbolTable.deleteContext();
  }
  [    LOOKAHEAD(1) "else"
    {
      symbolTable.newContext();
    }
    node2 = Statement()
    {
      symbolTable.deleteContext();
    }
  ]
  {
    return new IfStatementNode( t, expression, node1, node2);  }
}

WhileStatementNode WhileStatement():
{
  Token t;
  ExpressionNode condition;
  StatementNode execution;
}
{
  t="while" "("
  condition = Expression()
  ")"
  execution = Statement()

  {
    return new WhileStatementNode(condition, execution,
                                new CodeZoneClass(t,execution));  }
}

DoStatementNode DoStatement():
{
  Token t, t2;
  ExpressionNode condition;
  StatementNode execution;
}
{
  t = "do"
  execution = Statement()
  "while" "("
  condition = Expression()
  ")" t2 = ";"
  {
    return new DoStatementNode(execution, condition, new CodeZoneClass(t,t2,file));
  }
}

StatementNode ForStatement():
{
  Token t, t2;

  List<PrintInfo> out = new LinkedList<PrintInfo>();

  StatementNode init, update, execution;
  ExpressionNode condition;

  int mods;
  BaseType type;
  ExpressionNode iterable;

  StatementNode result;
  
  BlockCode.TmpBlockCode prevBlock = localBlock;
  symbolTable.newContext();
}
{
  t="for" "("
  {    localBlock = new BlockCode.TmpBlockCode(t,file);
  }

  (
      LOOKAHEAD(Modifiers() Type() <IDENTIFIER> ":")
      mods = Modifiers(out) type = Type() t2=<IDENTIFIER> ":"
      iterable = Expression()
      {
        LocalVariableSymbol it = new LocalVariableSymbol(mods, type, t2.image);
        symbolTable.addSymbol(it);
        localBlock.addLocalVariable(it);
      }

      ")" execution = Statement()

      {
        result = new IterableFor(it,
                                 iterable,
                                 execution,
                                 new CodeZoneClass(t, execution)
                                );      }
    |  // -------------------------------------------------------------------
      (        init = ForInit() ";"
      |
        t2 = ";"
        {
          init = new NoOpStatement(new CodeZoneClass(t2,t2,file));         }
      )

      (         condition = Expression() ";"
      |
        t2 = ";"
        {
          condition = new LiteralData.TrueLiteral(t2,file);        }
      )

      (        update = ForUpdate() ")"
      |
        t2 = ")"
        {
          update = new NoOpStatement(new CodeZoneClass(t2,t2,file));         }
      )
      execution = Statement()
      {
        result = new ClassicFor(localBlock.localVariables,
                                init,condition,update,execution,
                                new CodeZoneClass(t,execution));      }
  )

  {    localBlock = prevBlock;
    symbolTable.deleteContext();

    return result;
  }
}

StatementNode ForInit():
{
  StatementNode result;
}
{
  (
    LOOKAHEAD( Modifiers() Type() <IDENTIFIER> )
    result = LocalVariableDeclaration()
  |
    result = StatementExpressionList()
  )
  {    return result;
  }
}

StatementNode StatementExpressionList():
{
  StatementNode statement;
  List<StatementNode> statements = new LinkedList<StatementNode>();
}
{
  statement = StatementExpression()
  {    statements.add(statement);
  }
  ( ","
    statement = StatementExpression()
    {
      statements.add(statement);      
    }
  )*
  {
    statement = new StatementList(statements.toArray(new StatementNode[statements.size()]));
    
    return statement;  }
}

StatementNode ForUpdate():
{
  StatementNode aux;
}
{
  aux = StatementExpressionList()
  {    return aux;
  }
}

JumpStatement BreakStatement():
{
  Token t = null, t2, t3;
}
{
  t2="break" [ t=<IDENTIFIER> ] t3=";"
  {
    return new JumpStatement( JumpStatement.Operator.BREAK,
                              (t == null)? null : t.image,
                              new CodeZoneClass(t2,t3,file));                                
  }
}

JumpStatement ContinueStatement():
{
  Token t = null, t2, t3;
}
{
  t2 = "continue" [ t=<IDENTIFIER> ] t3 = ";"
  {
    return new JumpStatement( JumpStatement.Operator.CONTINUE,
                              (t == null)? null : t.image,
                              new CodeZoneClass(t2,t3,file));
                                
  }
}

ReturnStatementNode ReturnStatement():
{
  Token t, t2;
  ExpressionNode exp = null;
}
{
  t="return"
  [ exp=Expression() ] t2=";"
  {
    return new ReturnStatementNode(exp, new CodeZoneClass(t, t2,file));
  }
}

ThrowStatementNode ThrowStatement():
{
  Token t, t2;
  ExpressionNode exp = null;
}
{
  t="throw" exp = Expression() t2=";"
  {
    return new ThrowStatementNode(exp, new CodeZoneClass(t, t2,file));
  }
}

SynchronizedStatementNode SynchronizedStatement():
{
  Token t;
  ExpressionNode exp;
  BlockCode block;
}
{
  t="synchronized" "("
  exp = Expression()
  ")"
  block = Block()
  {
    return new SynchronizedStatementNode(exp, block,
                                        new CodeZoneClass(t,block));  }
}

TryStatementNode TryStatement():
/*
 * Semantic check required here to make sure that at least one
 * finally/catch is present.
 */
{
  ParameterClass param;
  Token t;
  CodeZone cz;

  BlockCode execution, aux, finalment = null;
  List<TryStatementNode.CatchBlock> catches = new LinkedList<TryStatementNode.CatchBlock>();
}
{
  t = "try"
  execution = Block()
  {
    cz = new CodeZoneClass(t, execution);  }
  ( "catch" "("
    param = FormalParameter()
    ")"
    {
      LocalVariableSymbol symbol = new LocalVariableSymbol(param.modifiers,
	                                                       param.type,
	                                                       param.name);
      symbolTable.newContext();
      symbolTable.addSymbol(symbol);
    }
    aux = Block()
    {
      symbolTable.deleteContext();
      catches.add(new TryStatementNode.CatchBlock(symbol, aux));
      cz = new CodeZoneClass(cz, aux);    }
  )*
  [ "finally"
    finalment = Block()
    {
      cz = new CodeZoneClass(cz, finalment);    }
  ]
  {
    return new TryStatementNode(execution, catches, finalment, cz);  }
}

/* We use productions to match >>>, >> and > so that we can keep the
 * type declaration syntax with generics clean
 */

void RUNSIGNEDSHIFT():
{}
{
  ( LOOKAHEAD({ getToken(1).kind == GT &&
                ((cat.quadriga.parsers.tokens.MySimpleToken)getToken(1)).realKind == RUNSIGNEDSHIFT} )
   ">" ">" ">"
  )
}

void RSIGNEDSHIFT():
{}
{
  ( LOOKAHEAD({ getToken(1).kind == GT &&
                ((cat.quadriga.parsers.tokens.MySimpleToken)getToken(1)).realKind == RSIGNEDSHIFT} )
  ">" ">"
  )
}

/* Annotation syntax follows. */

void Annotation(List<PrintInfo> out):
{}
{
   LOOKAHEAD( "@" Name() "(" ( <IDENTIFIER> "=" | ")" ))
   NormalAnnotation(out)
 |
   LOOKAHEAD( "@" Name() "(" )
   SingleMemberAnnotation(out)
 |
   MarkerAnnotation(out)
}

void NormalAnnotation(List<PrintInfo> out):
{
}
{
   "@"
   {
     out.add(new PrintString("@"));
   }
   Name()
   "("
   {
     out.add(PrintWhiteSpace.getInstance());
     out.add(new PrintString("("));
   }
   [ MemberValuePairs(out) ] ")"
   {
     out.add(new PrintString(")"));
   }
}

void MarkerAnnotation(List<PrintInfo> out):
{}
{
  "@"
   {
     out.add(new PrintString("@"));
   }
   Name()
}

void SingleMemberAnnotation(List<PrintInfo> out):
{}
{
  "@"
   {
     out.add(new PrintString("@"));
   }
   Name() "("
   {
     out.add(PrintWhiteSpace.getInstance());
     out.add(new PrintString("("));
   }
   MemberValue(out)
   ")"
   {
     out.add(new PrintString(")"));
   }
}

void MemberValuePairs(List<PrintInfo> out):
{}
{
   MemberValuePair(out)
   ( ","
     {
       out.add(PrintWhiteSpace.getInstance());
       out.add(new PrintString(","));
     }
     MemberValuePair(out)
   )*
}

void MemberValuePair(List<PrintInfo> out):
{
  Token t;
}
{
   t=<IDENTIFIER> "="
   {
     out.add(new PrintString(t.image));
     out.add(PrintWhiteSpace.getInstance());
     out.add(new PrintString("="));
     out.add(PrintWhiteSpace.getInstance());
   }
   MemberValue(out)
}

void MemberValue(List<PrintInfo> out):
{}
{
   Annotation(out)
 |
   MemberValueArrayInitializer(out)
 |
   ConditionalExpression()
}

void  MemberValueArrayInitializer(List<PrintInfo> out):
{}
{
  "{"
  {
    out.add(new PrintString("{"));
    out.add(PrintInitTab.getInstance());
    out.add(PrintIntro.getInstance());
  }
  ( MemberValue(out)
    ( LOOKAHEAD(2) ","
    {
      out.add(new PrintString("}"));
      out.add(PrintWhiteSpace.getInstance());
    }
      MemberValue(out)
    )* [ "," ]
  )?
  "}"
  {
    out.add(PrintEndTab.getInstance());
    out.add(PrintIntro.getInstance());
    out.add(new PrintString("}"));
  }
}


/* Annotation Types. */

void AnnotationTypeDeclaration(int modifiers, List<PrintInfo> out):
{
  Token t;
}
{
  "@" "interface" t=<IDENTIFIER>
  {
    out.add(new PrintString("@"));
    out.add(PrintWhiteSpace.getInstance());
    out.add(new PrintString("interface"));
    out.add(PrintWhiteSpace.getInstance());
    out.add(new PrintString(t.image));
    out.add(PrintWhiteSpace.getInstance());
  }
  AnnotationTypeBody(out)
}

void AnnotationTypeBody(List<PrintInfo> out):
{}
{
  "{"
  {
    out.add(new PrintString("{"));
    out.add(PrintInitTab.getInstance());
    out.add(PrintIntro.getInstance());
  }
  ( AnnotationTypeMemberDeclaration(out) )*
  "}"
  {
    out.add(PrintEndTab.getInstance());
    out.add(PrintIntro.getInstance());
    out.add(new PrintString("}"));
  }
}

void AnnotationTypeMemberDeclaration(List<PrintInfo> out):
{
   int modifiers;
   Token t;
}
{
 modifiers = Modifiers(out)
 (
   LOOKAHEAD(Type() <IDENTIFIER> "(")
   Type() t=<IDENTIFIER> "(" ")"
   {
     out.add(new PrintString(t.image));
     out.add(new PrintString("("));
     out.add(new PrintString(")"));
   }
   [ DefaultValue(out) ] ";"
   {
     out.add(new PrintString(";"));
   }
  |
   ClassOrInterfaceDeclaration(modifiers, out)
  |
   EnumDeclaration(modifiers, out)
  |
   AnnotationTypeDeclaration(modifiers, out)
  |
   FieldDeclaration(modifiers, out)
 )
 |
   ( ";" )
}

void DefaultValue(List<PrintInfo> out):
{}
{
  "default"
  {
     out.add(new PrintString("default"));
     out.add(PrintWhiteSpace.getInstance());
  }
  MemberValue(out)
}
