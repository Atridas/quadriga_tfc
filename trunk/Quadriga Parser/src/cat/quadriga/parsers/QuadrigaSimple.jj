void ThreadDeclaration():
{
  Token t;
  List<Token> name;
  QuadrigaThread thread;
  List<QuadrigaSystem> systems = new LinkedList<QuadrigaSystem>();
  Set<String> systemNames = new HashSet<String>();
  BlockCode block = null;}
{
  "@thread"
  (
    LOOKAHEAD(2)
    t = < IDENTIFIER >
    "{"
        (
          "@system" name = Name()
          {
            String n = Utils.getName(name);
            BaseSymbol symbol = symbolTable.findSymbol(n);
            if(symbol instanceof TypeSymbol
               && ((TypeSymbol)symbol).type instanceof QuadrigaSystem)
            {
              QuadrigaSystem system = (QuadrigaSystem)((TypeSymbol)symbol).type;
              if(systemNames.contains(system.getBinaryName())) {
                errorLog.insertError("El sistema " + n + " està repetit",
                                   new CodeZoneClass(name.get(0),name.get(name.size()-1),file));              } else              {                systemNames.add(system.getBinaryName());
                systems.add(system);
              }            } else {
              errorLog.insertError("El símbol " + n + " no és un sistema",
                                   new CodeZoneClass(name.get(0),name.get(name.size()-1),file));            }          }        )+
        [ "@init" block = Block() ]
        {
          thread = new CompleteThread(currentFile.replace('/','.'),t.image,
                                      systems, block,file);        }
    "}"
  |
    name = Name()
    {
      String res = "";
      String dependency = null;
      for(int i = 0; i < name.size(); i++) {
        if(i == name.size() - 1)
        {
          dependency = res;
        }
        if(i != 0) {
          res += "." + name.get(i).image;
        } else {
          res = name.get(i).image;
        }
      }
      if(name.size() == 1)
      {
        thread = new ProxyThread(currentFile.replace('/','.') + '.' + res);
      } else
      {
        BaseSymbol symbol = symbolTable.findSymbol(res);
        if(symbol != null) {
          if(symbol instanceof TypeSymbol
             && ((TypeSymbol)symbol).type instanceof QuadrigaThread)
          {
            thread = null; //Ja hi és a la taula de símbols
          } else
          {
            thread = new ProxyThread(res);
            errorLog.insertError("El símbol " + res + " no és un thread",
                                 new CodeZoneClass(name.get(0),name.get(name.size()-1),file));
          }
        } else {
          thread = new ProxyThread(res);
          addDependency(dependency);
        }
      }
    }  )
  {
    if(thread != null) {
      BaseSymbol symbol = new TypeSymbol(thread);
      symbolTable.addSymbol(symbol);
    }  }}

ComponentAllocation ComponentDeclarator():
{
  List<Token> name;
  CallToArguments args;}
{
  name = Name() args = Arguments()
  {
    CodeZone cz = new CodeZoneClass(name.get(0), args);
    
    String symbolName = Utils.getName(name);
    BaseSymbol symbol = symbolTable.findSymbol(symbolName);
    if(symbol != null && symbol instanceof TypeSymbol) {
      BaseType type = ((TypeSymbol)symbol).type;
      if(type instanceof QuadrigaComponent) {
        CallToNamedArguments named;
        if(args instanceof CallToListedArguments)        {
          if(((CallToListedArguments)args).arguments.size() > 0) {
            errorLog.insertError("Component arguments must be named", cz);          }
          named = new CallToNamedArguments(Collections.<String,ExpressionNode>emptyMap(),args);        } else {          named = (CallToNamedArguments) args;
        }
        
        return new ComponentAllocation((QuadrigaComponent)type,named,cz);      } else {
        errorLog.insertError("Symbol " + symbolName + " is not a Component", cz);      }    } else {      errorLog.insertError("Symbol " + symbolName + " is not a Type", cz);
    }
    QuadrigaComponent component = new ProxyComponent(symbolName);
    CallToNamedArguments named = new CallToNamedArguments(Collections.<String,ExpressionNode>emptyMap(),args);
    return new ComponentAllocation(component,named,cz);  }}

ApplyPrototypeToEntity EntityInitialization(ExpressionNode entity):
{
  List<Token> name;
  CodeZone cz, argsZone;
  Token t;
  Map<String,ExpressionNode> namedArguments = new HashMap<String,ExpressionNode>();
  Map<QuadrigaComponent,CallToNamedArguments> componentArguments = new HashMap<QuadrigaComponent,CallToNamedArguments>();}
{
  name=Name() t="("
  {
    cz = new CodeZoneClass(name.get(0), t,file);
    argsZone = new CodeZoneClass(t,file);
    String prototypeName = Utils.getName(name);
    BaseSymbol symbol = symbolTable.findSymbol(prototypeName);
    QuadrigaPrototype prototype;
    if(symbol != null && symbol instanceof TypeSymbol) {
      BaseType type = ((TypeSymbol)symbol).type;
      if(type instanceof QuadrigaPrototype) {
        prototype = ((QuadrigaPrototype)type);      } else {
        errorLog.insertError("Symbol " + prototypeName + " is not a Prototype",cz);
        prototype = new ProxyPrototype(prototypeName);
        symbolTable.addSymbol(new TypeSymbol(prototype));      }    } else {
      errorLog.insertError("Symbol " + prototypeName + " is not a Type",cz);
      prototype = new ProxyPrototype(prototypeName);
      symbolTable.addSymbol(new TypeSymbol(prototype));    }  }
  [  PrototypeArgument(namedArguments,componentArguments)
     ("," PrototypeArgument(namedArguments,componentArguments))*
  ]
  t=")"
  {
    cz = new CodeZoneClass(cz, t);
    argsZone = new CodeZoneClass(argsZone,t);
    return new ApplyPrototypeToEntity(
                        entity,
                        prototype,
                        new CallToNamedArguments(namedArguments,argsZone),
                        componentArguments,
                        cz);  }}

void PrototypeArgument(Map<String,ExpressionNode> namedArguments, Map<QuadrigaComponent,CallToNamedArguments> componentArguments):
{
  ExpressionNode expression;
  Token t;}{
  LOOKAHEAD(< IDENTIFIER > ":")
  t=< IDENTIFIER > ":" expression=Expression()
  {
    namedArguments.put(t.image, expression);
  }
| Name() ":" Expression()}

QuadrigaFunction QuadrigaUpdate():
{
  List<ParameterClass > parameters;

  
  symbolTable.newContext();

  BlockCode.TmpBlockCode prevBlock = localBlock;
  
  BlockCode code;
  Token t;}{
  t="@update"
  { localBlock = new BlockCode.TmpBlockCode(t,file); }
  parameters = FormalParameters()
  code = Block()

  {
    localBlock.addStatement(code);
    localBlock.setEndCode(code);
    code = localBlock.transformToBlockCode();
    localBlock = prevBlock;
    symbolTable.deleteContext();

    return new QuadrigaFunction(parameters, code, code);  }}